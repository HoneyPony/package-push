resource_local_to_scene = true
code = "shader_type canvas_item;
render_mode blend_mix;

uniform sampler2D anim;
uniform float time;

void fragment() {
	
	
	float t2 = min(time, 0.97);
	float frame = round(t2 * 24.0) / 24.0;
	
	vec2 uv = UV;
	uv.x = fract(uv.x);
	uv.y = fract(uv.y);
	
	uv.x /= 24.0;
	uv.x += frame;
	COLOR = texture(anim, uv);
}"
